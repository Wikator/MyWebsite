<div class="row featurette">
  <div class="col-md-7">
    <h2 class="featurette-heading fw-normal lh-1"><%= title %></h2>
    <p class="lead"><%= text %></p>
      <br />
      <% links.each do |link| %>
        <a href="<%= link[:url] %>"><%= link[:title] %></a><br />
      <% end %>
  </div>
  <div class="col-md-5">
    <%= image_tag image, width: 500, height: 500, class: "img-fluid mx-auto" %>
  </div>
</div>
